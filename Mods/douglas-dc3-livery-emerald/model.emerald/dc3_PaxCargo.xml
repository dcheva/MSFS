    <Component ID="starter1upd">
        <UseTemplate Name="ASOBO_GT_Update">
            <FREQUENCY>10</FREQUENCY>
                <UPDATE_CODE>
                    (A:RECIP ENG STARTER TORQUE:1, enum) 0 &gt; if{
                        1 (&gt;L:START_CHECK_1, number)
                    }
                    (A:GENERAL ENG COMBUSTION:1, bool) 1 == (L:START_CHECK_1) 1 == and if{
                        (L:exhaust_l_count) 1 + (&gt;L:exhaust_l_count)
                    }
                    (L:START_CHECK_1) 1 == (L:exhaust_l_count) 50 &gt; and if{
                        0 (&gt;L:START_CHECK_1, number)
                        0 (&gt;L:exhaust_l_count, number)
                    }
                    (L:mesh_Starter, number) 0 != (L:SWITCH_starter, number) 0 != and (L:exhaust_l_count) 0 &gt; and (A:GENERAL ENG COMBUSTION:1, bool) 0 == and if{
                        0 (&gt;L:exhaust_l_count, number)
                    }


                    (A:RECIP ENG STARTER TORQUE:2, enum) 0 &gt; if{
                        1 (&gt;L:START_CHECK_2, number)
                    }
                    (A:GENERAL ENG COMBUSTION:2, bool) 1 == (L:START_CHECK_2) 1 == and if{
                        (L:exhaust_r_count) 1 + (&gt;L:exhaust_r_count)
                    }
                    (L:START_CHECK_2) 1 == (L:exhaust_r_count) 50 &gt; and if{
                        0 (&gt;L:START_CHECK_2, number)
                        0 (&gt;L:exhaust_r_count, number)
                    }
                    (L:mesh_Starter, number) 2 != (L:SWITCH_starter, number) 2 != and (L:exhaust_r_count) 0 &gt; and (A:GENERAL ENG COMBUSTION:2, bool) 0 == if{
                        0 (&gt;L:exhaust_r_count, number)
                    }
                </UPDATE_CODE>
        </UseTemplate>
    </Component>

